BUILD_DIR       := build
OBJ_DIR         := $(BUILD_DIR)/obj32
EXE_DIR         := $(BUILD_DIR)/exe32
CC              := gcc -m32 -march=i686 -mtune=i686
CXX             := gcc -m32 -march=i686 -mtune=i686
MAKEFILE        := makefile32
CNEXT_OBJ_DIR   := lib/cnext/build/obj32

include include.mk

clean:
	$(REMOVE) $(OBJ_DIR)
	$(MAKE) -C lib/cnext -f makefile32 clean

